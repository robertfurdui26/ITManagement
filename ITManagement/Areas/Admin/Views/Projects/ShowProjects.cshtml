@model List<Project>
<div class="container-fluid">
    <div class="card shadow border-0 my-4">
        <div class="card-header bg-dark ml-0 py-3">
            <div class="row">
                <div class="col-12 text-center">
                    <h2 class="text-white py-2">Projects</h2>
                </div>
            </div>
        </div>
        <div class="card-body p-4">
            <div class="row pb-3">
                <div class="col-6">
                </div>
                <div class="col-6 text-end">
                    <a asp-controller="Projects" asp-action="Upsert" class="btn btn-dark">
                        <i class="bi bi-plus-circle"></i> Add new Project
                    </a>
                </div>
            </div>

            <div class="table-responsive">
                <table id="tblData" class="table table-bordered w-100" style="width:100%">
                    <thead>
                        <tr class="text-center">
                            <th>Name</th>
                            <th>Description</th>
                            <th>Finish Date</th>
                            <th>Project Leader</th>
                           
                            <th></th>
                        </tr>
                    </thead>
                    <!-- Restul codului tabelului -->
                </table>
            </div>
        </div>
    </div>
</div>

@section Scripts{
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script src="~/js/projects.js"></script>
}